{
	"config_version": "4",
	"name": "checkiday-mobile",
	"author": "kevzettler@gmail.com",
	"version": "0.1",
	"platform_version": "v2.0.4",
	"description": "A list of today's holidays, most of which you won't find on a calendar!",
	"core": {
		"general": {
			"logging": {
				"level": "DEBUG"
			},
			"reload": true
		},
		"android": {
			"minimum_version": "5"
		},
		"ios": {
			"minimum_version": "4.3",
			"device_family": "any"
		}
	},
	"modules": {
		"request": {
			"version": "2.3",
			"config": {
				"permissions": [
					"http://www.checkiday.com/api/*"
				]
			}
		},
		"prefs": {
			"version": "2.0"
		},
		"icons": {
			"version": "2.1",
			"config": {
				"ios": {
					"57": "img/icons/ios/57.png",
					"72": "img/icons/ios/72.png",
					"76": "img/icons/ios/76.png",
					"114": "img/icons/ios/114.png",
					"120": "img/icons/ios/120.png",
					"144": "img/icons/ios/144.png",
					"152": "img/icons/ios/152.png",
					"512": "img/icons/ios/512.png"
				}
			}
		},
		"launchimage": {
			"version": "2.1",
			"config": {}
		}
	},
	"homepage": "www.checkiday.com"
}